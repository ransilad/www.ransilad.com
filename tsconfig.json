{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "isolatedModules": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@icons/*": [
        "src/icons/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}